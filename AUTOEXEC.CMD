REM Program Files (x86) Alias
SET ProgramFiles(x86)=%ProgramFiles%
IF NOT "%PROCESSOR_ARCHITECTURE%" == "x86" SET ProgramFiles(x86)=%ProgramFiles% (x86)

REM Force Set Processor Architecture
REM SET PROCESSOR_ARCHITECTURE=x86
REM ECHO Processor Architecture is now %PROCESSOR_ARCHITECTURE%

REM Set Linux Processor Architecture
IF "%PROCESSOR_ARCHITECTURE%" == "x86" SET LIN_PROCESSOR_ARCHITECTURE=i686
IF "%PROCESSOR_ARCHITECTURE%" == "AMD64" SET LIN_PROCESSOR_ARCHITECTURE=x86_64
