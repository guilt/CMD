SET USER=%USERNAME%

REM Application Data
SET AppDataLocal=%HOME%\AppData\Local
SET AppDataLocalLow=%HOME%\AppData\LocalLow
SET AppDataRoaming=%HOME%\AppData\Roaming
SET AppDataRemote=%HOME%\AppData\Remote
