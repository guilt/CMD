REM Detect Bits
IF "%PROCESSOR_ARCHITECTURE%" == "x86" SET BITS=32
IF "%PROCESSOR_ARCHITECTURE%" == "AMD64" SET BITS=64
REM ECHO Bits is now %BITS%

REM Detect Version
SET WINVER=2000
FOR /F "TOKENS=4-5 DELIMS=[.]" %%I in ('VER') DO SET WINVER=%%I
REM ECHO Windows Version is %WINVER%

REM Prompt
SET PROMPT=%USER%@%COMPUTERNAME% (%PROCESSOR_ARCHITECTURE%) $P$G
IF NOT "%WINVER%" == "2600" SET PROMPT=$E[92;44m%USER%@%COMPUTERNAME%$E[91;44m (%PROCESSOR_ARCHITECTURE%) $E[97;44m$P$G$E[m

